<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phonebook2.PhoneBookController">
    <children>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <ListView fx:id="bookList" prefHeight="200.0" prefWidth="200.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
            <children>
                <TextField fx:id="nameTF" />
                <TextField fx:id="numberTF" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
            <children>
                <Button fx:id="addNumberButton" mnemonicParsing="false" onAction="#addNumber" text="add number" />
                <Button fx:id="getNumberButton" mnemonicParsing="false" onAction="#getNumber" text="get number" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="86.0" prefWidth="174.0">
            <children>
                <TextField fx:id="resultTF" editable="false" />
            </children>
        </HBox>
    </children>
</VBox>
